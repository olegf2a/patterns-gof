{
  "name": "vendor_name/app",
  "description": "PHP 8.4 project in Docker",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "leo",
      "email": "email@example.com"
    }
  ],
  "require": {
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-soap": "*",
    "goetas-webservices/soap-client": "*",
    "symfony/http-client": "^7.2",
    "nyholm/psr7": "^1.8",
    "laminas/laminas-zendframework-bridge": "^1.3"
  },
    "require-dev": {
        "goetas-webservices/wsdl2php": "*",
        "goetas-webservices/wsdl-reader": "*"
    },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
